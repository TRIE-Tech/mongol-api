{
  "name": "mongol-api-images-service",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/mongol-api-images-service",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/mongol-api-images-service"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/mongol-api-images-service"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "mongol-api-images-service:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "mongol-api-images-service:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "mongol-api-images-service:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "mongol-api-images-service:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/mongol-api-images-service/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/mongol-api-images-service/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "export-env": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "[ -f apps/mongol-api-images-service/.env ] && export $(cat apps/mongol-api-images-service/.env | xargs) || true"
        ],
        "parallel": false
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "rm -rf dist/apps/mongol-api-images-service",
          "rm -rf .vercel"
        ]
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx dotenv -v VERCEL_ORG_ID=o9MY67XW3sHiB6lhFI2pyFpD -v VERCEL_PROJECT_ID=prj_vM7uzIBceSmothrbGbIQOCcB6qCF -- vercel pull --yes --environment=preview --token=$VERCEL_TOKEN",
          "vercel build --token=$VERCEL_TOKEN",
          "npx dotenv -v VERCEL_ORG_ID=o9MY67XW3sHiB6lhFI2pyFpD -v VERCEL_PROJECT_ID=prj_vM7uzIBceSmothrbGbIQOCcB6qCF -- vercel --prebuilt --token=$VERCEL_TOKEN"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "export-env"
        }
      ]
    },
    "deploy-prod": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx dotenv -v VERCEL_ORG_ID=o9MY67XW3sHiB6lhFI2pyFpD -v VERCEL_PROJECT_ID=prj_vM7uzIBceSmothrbGbIQOCcB6qCF -- vercel pull --yes --environment=production --token=$VERCEL_TOKEN",
          "vercel build --prod --token=$VERCEL_TOKEN",
          "npx dotenv -v VERCEL_ORG_ID=o9MY67XW3sHiB6lhFI2pyFpD -v VERCEL_PROJECT_ID=prj_vM7uzIBceSmothrbGbIQOCcB6qCF -- vercel --prebuilt --prod --token=$VERCEL_TOKEN"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "export-env"
        }
      ]
    }
  },
  "tags": []
}
